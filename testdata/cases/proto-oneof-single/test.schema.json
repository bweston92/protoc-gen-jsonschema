{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$ref": "#/$defs/.samples.Values",
  "$defs": {
    ".samples.Values": {
      "allOf": [
        {
          "oneOf": [
            {
              "required": [
                "string_value"
              ]
            },
            {
              "required": [
                "int_value"
              ]
            },
            {
              "required": [
                "bool_value"
              ]
            },
            {
              "required": [
                "double_value"
              ]
            },
            {
              "not": {
                "anyOf": [
                  {
                    "required": [
                      "string_value"
                    ]
                  },
                  {
                    "required": [
                      "int_value"
                    ]
                  },
                  {
                    "required": [
                      "bool_value"
                    ]
                  },
                  {
                    "required": [
                      "double_value"
                    ]
                  }
                ]
              }
            }
          ]
        }
      ],
      "properties": {
        "value": {
          "$ref": "#/$defs/.samples.Values.value"
        },
        "count": {
          "$ref": "#/$defs/.samples.Values.count"
        },
        "string_value": {
          "$ref": "#/$defs/.samples.Values.string_value"
        },
        "int_value": {
          "$ref": "#/$defs/.samples.Values.int_value"
        },
        "bool_value": {
          "$ref": "#/$defs/.samples.Values.bool_value"
        },
        "double_value": {
          "$ref": "#/$defs/.samples.Values.double_value"
        }
      },
      "type": "object",
      "required": [
        "value",
        "count"
      ]
    },
    ".samples.Values.value": {
      "type": "string"
    },
    ".samples.Values.count": {
      "type": "integer"
    },
    ".samples.Values.string_value": {
      "type": "string"
    },
    ".samples.Values.int_value": {
      "type": "integer"
    },
    ".samples.Values.bool_value": {
      "type": "boolean"
    },
    ".samples.Values.double_value": {
      "type": "number"
    }
  },
  "type": "object"
}